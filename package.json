{
  "name": "@a11ywatch/elastic-cdn",
  "version": "0.5.5",
  "description": "cdn server for a11ywatch needs",
  "main": "./server.js",
  "author": "Jeffrey Mendez <jeff@a11ywatch.com>",
  "scripts": {
    "test": "jest",
    "build": "tsc",
    "dev": "ts-node-dev --transpile-only src/server.ts",
    "start": "NODE_ENV=production node dist/server.js",
    "pub": "tsc && cp package.json dist && cp package-lock.json dist && cp README.md dist && cp LICENSE dist && cd dist && npm publish"
  },
  "dependencies": {
    "@a11ywatch/protos": "^0.3.6",
    "@grpc/grpc-js": "1.7.3",
    "@grpc/proto-loader": "0.7.3",
    "@swc/core": "^1.3.21",
    "aws-sdk": "^2.1265.0"
  },
  "license": "MIT",
  "devDependencies": {
    "@swc/jest": "^0.2.20",
    "@types/aws-sdk": "^2.7.0",
    "@types/node": "^14.11.2",
    "jest": "^26.4.2",
    "prettier": "^2.0.5",
    "supertest": "^5.0.0",
    "ts-node-dev": "^1.1.6",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.7.0"
  },
  "jest": {
    "moduleNameMapper": {
      "^@app/(.*)$": "<rootDir>/src/$1"
    },
    "transform": {
      "^.+\\.(t|j)sx?$": "@swc/jest"
    },
    "verbose": true,
    "coverageDirectory": "./coverage/",
    "collectCoverage": true
  },
  "keywords": [
    "accessibility",
    "testing",
    "WCAG"
  ]
}
